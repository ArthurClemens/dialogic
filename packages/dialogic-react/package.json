{
  "name": "dialogic-react",
  "version": "0.6.0",
  "description": "Logic for dialogs and notifications",
  "main": "dist/dialogic-react",
  "module": "dist/dialogic-react.mjs",
  "type": "module",
  "scripts": {
    "npm-check-updates": "../../node_modules/npm-check-updates/bin/ncu -u",
    "clean": "rm -rf dist/*",
    "build": "npm run clean && npm run build:module && npm run build:umd && npm run size",
    "build:module": "TYPESCRIPT=1 ENTRY=src/index.ts MODULE=1 MODULE_NAME=dialogicReact ../../node_modules/rollup/dist/bin/rollup -c ../../scripts/rollup.config.js",
    "build:umd": "TYPESCRIPT=1 ENTRY=src/index.ts MODULE=0 MODULE_NAME=dialogicReact ../../node_modules/rollup/dist/bin/rollup -c ../../scripts/rollup.config.js",
    "size": "size-limit",
    "size:why": "size-limit --why"
  },
  "size-limit": [
    {
      "path": "dist/dialogic-react.js"
    }
  ],
  "dependencies": {
    "dialogic": "^0.6.0",
    "use-stream": "^0.3.6"
  },
  "peerDependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.5.0",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.5",
    "size-limit": "^4.5.0"
  },
  "files": [
    "dist",
    "index.d.ts",
    "README.md"
  ],
  "author": "Arthur Clemens <arthurclemens@gmail.com> (http://arthurclemens.com)",
  "homepage": "https://github.com/ArthurClemens/dialogic",
  "license": "MIT"
}
