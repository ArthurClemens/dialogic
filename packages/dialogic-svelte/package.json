{
  "name": "dialogic-svelte",
  "version": "0.2.0",
  "description": "Logic for dialogs and notifications",
  "main": "dist/dialogic-svelte",
  "module": "dist/dialogic-svelte.mjs",
  "scripts": {
    "npm-check-updates": "../../node_modules/npm-check-updates/bin/ncu -u",
    "clean": "rm -rf dist/*",
    "rollup": "MODULE_NAME=dialogicSvelte ENTRY=./src/index.js ../../node_modules/rollup/dist/bin/rollup -c ../../scripts/rollup.config.svelte.js && ENTRY=./src/index.js ../../node_modules/rollup/dist/bin/rollup -c ../../scripts/rollup.config.svelte.js ",
    "build": "npm run clean && npm run rollup && npm run size",
    "size": "gzip -c dist/dialogic-svelte.js | wc -c"
  },
  "dependencies": {
    "dialogic": "^0.2.0"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "svelte": "src/index.js",
  "author": "Arthur Clemens <arthurclemens@gmail.com> (http://arthurclemens.com)",
  "homepage": "https://github.com/ArthurClemens/dialogic",
  "license": "MIT"
}
