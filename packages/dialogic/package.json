{
  "name": "dialogic",
  "version": "0.0.1",
  "description": "Logic for dialogs and notifications",
  "main": "dist/dialogic",
  "module": "dist/dialogic.mjs",
  "scripts": {
    "clean": "rm -rf dist/*",
    "rollup": "TYPESCRIPT=1 MODULE=1 ../../node_modules/rollup/bin/rollup -c ../../scripts/rollup.config.js && TYPESCRIPT=1 ../../node_modules/rollup/bin/rollup -c ../../scripts/rollup.config.js ",
    "build": "npm run clean && npm run rollup && npm run size",
    "size": "gzip -c dist/dialogic.js | wc -c"
  },
  "dependencies": {
    "mithril": "^2.0.3"
  },
  "devDependencies": {
    "@types/mithril": "2.0.0",
    "@types/uuid": "^3.4.5"
  },
  "files": [
    "dist",
    "index.d.ts",
    "README.md"
  ],
  "author": "Arthur Clemens <arthurclemens@gmail.com> (http://arthurclemens.com)",
  "homepage": "https://github.com/ArthurClemens/dialogic",
  "license": "MIT"
}
