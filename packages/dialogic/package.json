{
  "name": "dialogic",
  "version": "0.3.1",
  "description": "Logic for dialogs and notifications",
  "main": "dist/dialogic",
  "module": "dist/dialogic.js",
  "scripts": {
    "npm-check-updates": "../../node_modules/npm-check-updates/bin/ncu -u",
    "clean": "rm -rf dist/*",
    "rollup": "TYPESCRIPT=1 ENTRY=src/index.ts MODULE=1 MODULE_NAME=dialogic ../../node_modules/rollup/dist/bin/rollup -c ../../scripts/rollup.config.js",
    "build": "npm run clean && npm run rollup"
  },
  "dependencies": {
    "mithril": "^2.0.4",
    "@types/mithril": "2.0.2"
  },
  "files": [
    "dist",
    "index.d.ts",
    "README.md"
  ],
  "author": "Arthur Clemens <arthurclemens@gmail.com> (http://arthurclemens.com)",
  "homepage": "https://github.com/ArthurClemens/dialogic",
  "license": "MIT"
}
