  "dependencies": {
    "dialogic": "0.13.10",
    "mithril-hooks": "^0.7.1"
  },
  "devDependencies": {
    "@types/mithril": "2.0.14",
    "dialogic-hooks": "0.13.10",
    "typescript": "^5.0.0",
    "vite": "^4.1.4"
  },
  "peerDependencies": {
    "mithril": "^2.0.4"
  },
  "author": "Arthur Clemens <arthurclemens@gmail.com> (http://arthurclemens.com)",
