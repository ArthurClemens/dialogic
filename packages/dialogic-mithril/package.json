{
  "name": "dialogic-mithril",
  "version": "0.2.2",
  "description": "Logic for dialogs and notifications",
  "main": "dist/dialogic-mithril",
  "module": "dist/dialogic-mithril.mjs",
  "scripts": {
    "npm-check-updates": "../../node_modules/npm-check-updates/bin/ncu -u",
    "clean": "rm -rf dist/*",
    "rollup": "MODULE_NAME=dialogicMithril TYPESCRIPT=1 ../../node_modules/rollup/dist/bin/rollup -c ../../scripts/rollup.umd.js && TYPESCRIPT=1 ../../node_modules/rollup/dist/bin/rollup -c ../../scripts/rollup.es.js ",
    "build": "npm run clean && npm run rollup && npm run size",
    "size": "gzip -c dist/dialogic-mithril.js | wc -c"
  },
  "dependencies": {
    "dialogic": "^0.2.1"
  },
  "devDependencies": {
    "@types/mithril": "2.0.2",
    "mithril": "^2.0.4"
  },
  "files": [
    "dist",
    "index.d.ts",
    "README.md"
  ],
  "author": "Arthur Clemens <arthurclemens@gmail.com> (http://arthurclemens.com)",
  "homepage": "https://github.com/ArthurClemens/dialogic",
  "license": "MIT"
}
