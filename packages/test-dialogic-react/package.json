{
  "name": "tests-dialogic-react",
  "private": true,
  "description": "Test Dialogic with React",
  "version": "0.12.7",
  "license": "MIT",
  "scripts": {
    "deps": "../../node_modules/npm-check-updates/bin/cli.js -u",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview --port 3000",
    "test": "npm run build && start-server-and-test preview 3000 cypress:run",
    "test:i": "npm-run-all --parallel dev cypress:open",
    "cypress:run": "../../node_modules/cypress/bin/cypress run",
    "cypress:open": "../../node_modules/cypress/bin/cypress open",
    "prettier": "prettier --config ../../.prettierrc.json --write \"./{src,scripts}/**/*.{js,jsx,ts,tsx,ejs}\""
  },
  "dependencies": {
    "dialogic": "^0.12.7",
    "dialogic-react": "^0.12.7",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-router-dom": "5.3.0"
  },
  "devDependencies": {
    "@types/react": "^17.0.26",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-dom": "^5.3.0",
    "npm-run-all": "^4.1.5",
    "start-server-and-test": "^1.14.0"
  }
}
