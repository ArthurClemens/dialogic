{
  "name": "demo-dialogic-svelte",
  "private": true,
  "description": "Dialogic for Svelte demo",
  "version": "0.4.7",
  "license": "MIT",
  "scripts": {
    "npm-check-updates": "../../node_modules/npm-check-updates/bin/ncu -u",
    "build": "npm run clean && rollup -c ./rollup.config.js",
    "autobuild": "rollup -c ./rollup.config.js -w",
    "dev": "run-p start:dev autobuild",
    "start:dev": "sirv public --dev --port 3000",
    "clean": "rm -rf public/js/*",
    "serve": "http-server -c-1 -p 8080 public",
    "github": "npm run build && node scripts/github.js"
  },
  "dependencies": {
    "dialogic-svelte": "^0.4.7",
    "svelte": "^3.21.0"
  },
  "devDependencies": {
    "@pyoner/svelte-ts-preprocess": "^1.2.1",
    "@pyoner/svelte-types": "^3.4.4",
    "dialogic": "^0.4.7",
    "http-server": "^0.12.1",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.7.2",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-livereload": "^1.2.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^3.1.1",
    "rollup-plugin-svelte": "^5.2.1",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-typescript2": "^0.27.0",
    "sirv-cli": "^0.4.5",
    "start-server-and-test": "^1.11.0",
    "svelte-ts-preprocess": "^1.1.3"
  }
}
