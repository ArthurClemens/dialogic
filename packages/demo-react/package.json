{
  "name": "demo-dialogic-react",
  "private": true,
  "version": "0.0.1",
  "description": "Logic for dialogs and notifications",
  "main": "dist/demo-dialogic-react",
  "module": "dist/demo-dialogic-react.mjs",
  "scripts": {
    "npm-check-updates": "../../node_modules/npm-check-updates/bin/ncu -u",
    "clean": "rm -rf dist/js/*",
    "dev": "npm-run-all --parallel dev:watch dev:serve",
    "dev:serve": "ENTRY=./src/index.tsx ../../node_modules/webpack-dev-server/bin/webpack-dev-server.js --config ../../scripts/webpack.config.dev.js --disableHostCheck true --port 3200 --host 0.0.0.0",
    "dev:watch": "ENTRY=./src/index.tsx ../../node_modules/webpack/bin/webpack.js --watch --config ../../scripts/webpack.config.dev.js",
    "webpack": "ENTRY=./src/index.tsx ../../node_modules/webpack/bin/webpack.js --config ../../scripts/webpack.config.prod.js",
    "build": "npm run clean && npm run webpack",
    "serve": "http-server -p 5000 dist",
    "typecheck": "../../node_modules/typescript/bin/tsc --noEmit --strict --esModuleInterop src/index.ts"
  },
  "dependencies": {
    "dialogic-react": "0.0.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  },
  "devDependencies": {
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.8.5",
    "dialogic": "^0.0.1",
    "npm-run-all": "^4.1.5",
    "typescript": "^3.5.3",
    "typings": "^2.1.1"
  },
  "author": "Arthur Clemens <arthurclemens@gmail.com> (http://arthurclemens.com)",
  "homepage": "https://github.com/ArthurClemens/dialogic",
  "license": "MIT"
}
