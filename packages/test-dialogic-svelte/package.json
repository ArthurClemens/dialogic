{
  "name": "tests-dialogic-svelte",
  "private": true,
  "description": "Test Dialogic with Svelte",
  "version": "0.13.3",
  "license": "MIT",
  "main": "public/tests-dialogic-svelte",
  "scripts": {
    "deps": "../../node_modules/npm-check-updates/bin/cli.js -u",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview --port 3000",
    "test": "npm run build && start-server-and-test preview 3000 cypress:run",
    "test:i": "npm-run-all --parallel dev cypress:open",
    "cypress:run": "../../node_modules/cypress/bin/cypress run",
    "cypress:open": "../../node_modules/cypress/bin/cypress open",
    "prettier": "prettier --config ../../.prettierrc.json --write \"./{src,scripts}/**/*.{js,jsx,ts,tsx,ejs}\""
  },
  "dependencies": {
    "dialogic": "0.13.3",
    "dialogic-svelte": "0.13.3",
    "svelte": "^3.43.1",
    "svelte-spa-router": "^3.2.0"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.26",
    "@tsconfig/svelte": "^2.0.1",
    "npm-run-all": "^4.1.5",
    "start-server-and-test": "^1.14.0"
  }
}
