{
  "name": "tests-dialogic-svelte",
  "private": true,
  "description": "Test Dialogic with Svelte",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "npm-check-updates": "../../node_modules/npm-check-updates/bin/ncu -u",
    "lint": "eslint ./src",
    "build": "npm run clean && rollup -c ./rollup.config.js",
    "autobuild": "rollup -c ./rollup.config.js -w",
    "dev": "run-p start:dev autobuild",
    "start:dev": "sirv public --dev --port 3200",
    "clean": "rm -rf dist/js/*",
    "serve": "http-server -c-1 -p 8080 public",
    "test": "npm run test:cypress",
    "test:cypress": "npm run build && start-server-and-test serve 8080 cypress:run",
    "test:cypress:i": "npm run build && npm-run-all --parallel serve cypress:open",
    "cypress:run": "../../node_modules/cypress/bin/cypress run",
    "cypress:open": "../../node_modules/cypress/bin/cypress open"
  },
  "dependencies": {
    "dialogic": "0.0.1",
    "dialogic-svelte": "0.0.1",
    "svelte": "^3.12.1",
    "svelte-spa-router": "^1.1.1"
  },
  "devDependencies": {
    "@pyoner/svelte-ts-preprocess": "^1.2.1",
    "@pyoner/svelte-types": "^3.4.4",
    "http-server": "^0.11.1",
    "npm-run-all": "^4.1.5",
    "rollup-plugin-commonjs": "^10.0.2",
    "rollup-plugin-livereload": "^1.0.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-svelte": "^5.1.0",
    "rollup-plugin-terser": "^5.1.1",
    "rollup-plugin-typescript2": "^0.22.1",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup": "^1.18.0",
    "sirv-cli": "^0.4.4",
    "start-server-and-test": "^1.10.4",
    "svelte-ts-preprocess": "^1.1.3"
  }
}
